
<% content_for :header do %>
    <div class="block-header">
      <h2><%= AcademicYear.current %> Exam Round</h2>
    </div>
<% end %>

<% if current_user.admin? %>
    <%= render partial: '/dashboards/admin' %>
<% elsif current_user.has_role(User::APPLICANT) %>
    <%= render partial: 'dashboards/applicant' %>
<% elsif current_user.has_role(User::INSTITUTION) %>
    <%= render partial: 'dashboards/institution'  %>
<% end %>
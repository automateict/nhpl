<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>Sign In | NHPL</title>
    <!-- Favicon-->
  <%= favicon_link_tag 'favicon.ico' %>

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    </head>

<body class="login-page users">
<!-- Page Loader -->
<div class="page-loader-wrapper">
    <div class="loader">
        <div class="preloader">
            <div class="spinner-layer pl-red">
                <div class="circle-clipper left">
                    <div class="circle"></div>
                </div>
                <div class="circle-clipper right">
                    <div class="circle"></div>
                </div>
            </div>
        </div>
        <p>Please wait...</p>
    </div>
</div>
<!-- #END# Page Loader -->
<h2 class="logo">
  <center>
  <a><%= image_tag 'logomoh.png', size: '150x150' %></a>
  <a href="javascript:void(0);">
    <h3><font color="white">Health Professionals Competency Assessment & Licensure</font></h3></a>
  </center>
</h2>
<br/>
<ul class="right">
  <span style="padding-right: 20px">
  <%= link_to root_path do %>

              <span class="btn btn-default"><i class="material-icons">home</i> HOME</span>
          <% end %>
    </span>
  <span style="padding-right: 20px">
  <%= link_to home_check_exam_result_path do %>

      <span class="btn btn-default"><i class="material-icons">info_outline</i> CHECK LICENSING EXAM RESULT</span>
  <% end %>
  </span>
</ul><br/><br/>
<hr/>
    <div class="login-box">
      <div class="card">
        <div class="body">
      <%= render partial: 'shared/flash_messages' %>
                <%= yield %>
        </div>
      </div>
    </div>
</body>

</html>
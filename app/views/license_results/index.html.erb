<div class="header">
  <h2>
    License Results
  </h2>
</div>
<div class="body table-responsive">
  <table class="table table-bordered table-striped table-hover dataTable js-exportable">
  <thead>
    <tr>
      <th>Applicant</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @license_results.each do |license_result| %>
      <tr>
        <td><%= license_result.applicant %></td>
        <td><%= license_result.result %></td>
        <td><%= link_to 'Show', license_result %></td>
        <td><%= link_to 'Edit', edit_license_result_path(license_result) %></td>
        <td><%= link_to 'Destroy', license_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
  <br/>

<%= link_to 'New License Result', new_license_result_path %>
</div>